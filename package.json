{
  "name": "eta-vue",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-s type-check build-only",
    "preview": "vite preview --host",
    "run": "run-s build preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore",
    "format": "prettier --cache --write src/ doc/ index.html *.ts *.json *.md cypress/",
    "spell": "cspell --cache --quiet \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" \"doc/*\"",
    "spell-store": "cspell --cache --quiet --words-only --unique \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" > cspell-unknown-words.txt",
    "check": "run-p type-check format lint spell test-once",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "vitest --watch=true",
    "test-once": "vitest --watch=false --coverage"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.5",
    "@mdi/font": "^7.4.47",
    "echarts": "^5.6.0",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@mdi/js": "^7.4.47",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.2.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.0",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
