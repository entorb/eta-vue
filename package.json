{
  "name": "eta-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --host",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --cache --write src/ doc/ index.html *.ts *.json *.md cypress/",
    "spell": "cspell --cache --quiet \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" \"doc/*\"",
    "spell-store": "cspell --cache --quiet --words-only --unique \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" > cspell-unknown-words.txt",
    "check": "run-p format lint spell test-once",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "vitest --watch=true --coverage",
    "test-once": "vitest --watch=false"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.13",
    "@mdi/font": "7.4.47",
    "echarts": "^5.5.0",
    "vue": "^3.4.30",
    "vue-echarts": "^6.7.3",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.10"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.12.0",
    "husky": "^9.0.11",
    "eslint": "^8.53.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.2",
    "typescript": "~5.5.2",
    "unplugin-fonts": "^1.1.1",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.22"
  }
}
