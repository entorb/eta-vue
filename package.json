{
  "name": "eta-vue",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "run": "run-s build preview",
    "test": "vitest --watch=false",
    "test-cov": "vitest --watch=false --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --cache --fix --ignore-pattern .gitignore",
    "format": "prettier --cache --write src/ cypress/ docs/ *.html *.ts *.json *.md --log-level silent",
    "spell": "cspell --cache --quiet \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" \"docs/*\"",
    "spell-store": "cspell --cache --quiet --words-only --unique \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" > cspell-unknown-words.txt",
    "check": "run-p format lint type-check spell test"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.9",
    "@mdi/font": "^7.4.47",
    "echarts": "^5.6.0",
    "vue": "^3.5.25",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.6.4",
    "vuetify": "^3.11.3"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cspell": "^9.4.0",
    "cypress": "^15.7.1",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "~10.5.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "unplugin-fonts": "^1.4.0",
    "vite": "^7.2.7",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.8"
  }
}
